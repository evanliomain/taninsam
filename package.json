{
  "name": "taninsam",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [
    "taninsam",
    "functional",
    "utils",
    "utilities",
    "fp",
    "pure",
    "immutable"
  ],
  "sideEffects": false,
  "main": "dist/taninsam.umd.js",
  "module": "dist/taninsam.es5.js",
  "typings": "dist/types/taninsam.d.ts",
  "files": [
    "dist"
  ],
  "author": "Evan Liomain <evan.liomain@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/evanliomain/taninsam"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint --format codeFrame --project tsconfig.json 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild:compiled": "rimraf dist",
    "build:compiled": "tsc",
    "postbuild:compiled": "cpx \"./src/@types/**/*.d.ts\" \"./dist/types/@types\"",
    "build:bundle": "rollup -c rollup.config.ts",
    "postbuild:bundle": "rimraf compiled",
    "build:doc": "typedoc src/taninsam.ts",
    "start": "tsc -w & rollup -c rollup.config.ts -w",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ci": "vitest --config ./vitest.config.ts --no-cache --coverage",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "gen": "plop function",
    "stryker": "stryker run",
    "stryker:debug": "stryker run --fileLogLevel trace --logLevel debug ./stryker.config.mjs",
    "stryker:ci": "stryker run ./stryker.config.ci.mjs"
  },
  "lint-staged": {
    "{src,tools}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.2.6",
    "@stryker-mutator/vitest-runner": "^8.2.6",
    "@types/node": "^13.1.6",
    "@vitest/coverage-v8": "^1.4.0",
    "commitizen": "^4.3.0",
    "cpx2": "^2.0.0",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.0.2",
    "lint-staged": "^9.0.0",
    "plop": "^4.0.1",
    "prettier": "^1.4.4",
    "rimraf": "^3.0.0",
    "rollup": "^1.15.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "semantic-release": "^15.9.9",
    "stryker-cli": "^1.0.2",
    "tslint": "^5.4.3",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "^0.16.0",
    "typedoc-default-themes-bugfixed": "^1.0.0",
    "typescript": "^5.4.3",
    "validate-commit-msg": "^2.12.2",
    "vitest": "^1.4.0"
  },
  "dependencies": {}
}
